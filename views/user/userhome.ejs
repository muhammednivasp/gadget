        <!-- Start Featured Product-->
        <section class="bg-white">
            <div class="  p-5 ">
                <div class="row text-center py-5">
                    <div class="col-lg-6 m-auto">
                        <h1 class="h1 ">products</h1>
                        <p>
                            you can choose from here
                        </p>
                    </div>
                </div>
                <div class="row bg-white justify-content-center">

                    <% product.forEach((x,index)=> { %>
                        <% if(x && x.unlist==false && x.category && x.category.unlist==false) {%>
                           
                            <div class="col-12 col-md-4 mb-4">
                                <div class="card h-120 bg-dark">
                                    <a href="/singleproduct/<%= x._id %>">
                                        <img style="height:500px; " src="/images/<%= x.image[0] %>" class="card-img-top"
                                            alt="...">
                                    </a>
                                    <div class="card-body">
                                        <!-- <ul class="list-unstyled d-flex justify-content-between"> -->
                                        <!-- <li>
                                            <i class="text-warning fa fa-star"></i>
                                            <i class="text-warning fa fa-star"></i>
                                            <i class="text-warning fa fa-star"></i>
                                            <i class="text-muted fa fa-star"></i>
                                            <i class="text-muted fa fa-star"></i>
                                        </li> -->

                                        <!-- </ul> -->
                                        <div>
                                            <a href="" class="h2 text-decoration-none text-white">
                                                <%= x.productname %>
                                            </a>
                                        </div>
                                        <br>

                                        <ul class="list-unstyled d-flex justify-content-between">
                                            <li class=" " style="color: aliceblue;">Price:<%= x.price %>
                                            </li>
                                        </ul>
                                        <br>

                                        <div>
                                            <a href="" class="h4 text-decoration-none text-success">
                                                <%= x.category.category %>
                                            </a>
                                        </div>
                                        <p class="card-text text-white">
                                            <%= x.description %>
                                        </p>
                                        <!-- <p class="text-muted">Reviews (24)</p> -->

                                        <div>

                                            <!-- <button type="button" class="btn btn-success me-2"><a
                                                    class="text-decoration-none text-white" href="">Buy Now</a></button> -->



                                            <button class=" btn btn-primary"><a class="text-decoration-none text-white"
                                                    href="/singleproduct/<%= x._id %>">View Product</a></button>

                                            <!-- <button class=" btn btn-primary"><a class="text-decoration-none text-white"
                                                    href="/addtocart/____">View Cart</a></button> -->


                                        </div>
                                    </div>
                                </div>
                            </div>
                           
                            <% } %>
                                <% }) %>

                </div>
                <div class="row text-center pt-4">
                    <div class="col-lg-6 m-auto">
                        <button class=" btn btn-warning w-100"><a class="text-decoration-none text-white"
                                href="/allproducts">show more</a></button>
                    </div>
                </div>
            </div>
            <% if(typeof message!=="undefined" ){%>
                <h1 style="font-size:16px;color:rgb(207, 38, 16);">
                    <%=message %>
                </h1>
                <%}%>
        </section>
        <!-- End Featured Product -->

