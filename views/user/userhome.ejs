                <% categories.forEach((x,index)=> { %>
                    <% if(x && x.unlist==false) {%>
                    <div class="col-12 col-md-3 p-5 mt-3 bg-dark ">
                        <!-- <a href="#"><img src="/userAssets/images/gd w r5.png" class="rounded-circle img-fluid border"></a> -->
                        <a href="/filter/<%= x._id%>">
                            <h5 class="text-center text-light mt-3 mb-3">
                                <%= x.category %>
                            </h5>
                            <p class="text-center text-light"><a>
                                    <%= x.description %>
                                </a></p>
                            <br>
                        </a>
                    </div>
                    <% } %>
                    <% }) %>
