<%- include('../layout/admin/header.ejs')%>
  <%- include('../layout/admin/navbar.ejs')%>
    <%- include('../layout/admin/sidebar.ejs')%>

    <main id="main" class="main">
        <div class="page-wrapper">
            <div class="container">
              <!-- <div class="row page-titles">
                <div class="col-md-5 align-self-center">
                  <h3 class="text-themecolor">Sales Report</h3>
                </div>
              </div> -->
              <div class="pagetitle">
                <h1>Get Sales Report</h1>
                <nav>
                  <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="/admin/dashboard">Home</a></li>
                    <li class="breadcrumb-item active">Get Sales Report</li>
                  </ol>
                </nav>
              </div>
              <div class="col-lg-12 col-xlg-12 col-md-12">
                <div class="card" style="width: 90%; margin-top: 10vh; background-color: black;">
                  <!-- Tab panes -->
                  <div class="card-body" >
                    <form action="/admin/salesreport" class="form-horizontal form-material" method="post" id="report"  >
                  </div> <div id="alertpasswordUser1" class="alert alert-danger" role="alert" style="display: none;">
                  </div>
                  <div style="padding: 10vh;">
                      <div class="form-group" style="margin-top: 5vh;" >
                        <label class="col-md-12">From- Date</label>
                        <div class="col-md-8">
                            <input type="date" name="from" id="from"  class="form-control form-control-line" >
                          </div>
                      </div>
                      <div class="form-group"  style="margin-top: 5vh;" >
                        <label class="col-md-12">To Date</label>
                        <div class="col-md-8">
                            <input type="date" name="end" id="end"  class="form-control form-control-line">
                          </div>
                      </div>
                      <div class="form-group">
                        <div class="col-sm-12">
                          <% if(typeof message !=='undefined' ){ %>
                            <h5 style="color: rgb(206, 47, 47);">
                                <%= message %>
                            </h5>
                            <% } %>
                          <button type='submit' class="btn btn-success"  style="margin-top: 5vh; margin-bottom: 2vh;">Submit</button>
                        </div>
                      </div>
                      </div>
                    </form>
                
                </div>
                </div>
              </div>
              <!-- Column -->
            </div>
        </main>

        <%- include('../layout/admin/footer.ejs')%>